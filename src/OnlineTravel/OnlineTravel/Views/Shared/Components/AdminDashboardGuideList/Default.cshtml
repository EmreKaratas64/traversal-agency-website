@model List<Guide>
<div class="col-md-6 col-lg-4 order-2 mb-4">
                  <div class="card h-100">
                    <div class="card-header d-flex align-items-center justify-content-between">
                      <h5 class="card-title m-0 me-2">Guide List</h5>
                    </div>
                    <div class="card-body">
                      <ul class="p-0 m-0">
                          @foreach (var item in Model)
                {
                     <li class="d-flex mb-4 pb-1">
                          <div class="avatar flex-shrink-0 me-3">
                            <img src="@item.Image" alt="Guide" class="rounded" />
                          </div>
                          <div class="d-flex w-100 flex-wrap align-items-center justify-content-between gap-2">
                            <div class="me-2">
                              <small class="text-muted d-block mb-1">Travel Online</small>
                              <h6 class="mb-0">@item.Name</h6>
                            </div>
                            <div class="user-progress d-flex align-items-center gap-1">
                              <a href="@item.InstagramUrl" class="btn btn-sm btn-outline-warning text-dark">Instagram</a>
                            </div>
                          </div>
                        </li>
                }

